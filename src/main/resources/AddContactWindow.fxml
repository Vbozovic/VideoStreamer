<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="addContactWindow" prefHeight="147.0" prefWidth="283.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.AddContactController">
   <children>
      <Label layoutX="14.0" layoutY="53.0" text="Contact name:" />
      <TextField fx:id="contactNameField" layoutX="118.0" layoutY="48.0" />
      <Button fx:id="addContactButton" layoutX="91.0" layoutY="100.0" mnemonicParsing="false" onAction="#addContact" text="Add" />
      <Button fx:id="cancelButton" layoutX="183.0" layoutY="100.0" mnemonicParsing="false" onAction="#closeWindow" text="Cancel" />
   </children>
</AnchorPane>
